<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.blockb.beez.dao.MapDao">
<!-- 모든 가게로딩 -->
<select id="getStoreList" resultType="com.blockb.beez.dto.MapDto">
    select b.nickname storeName,a.lat, a.lon  from store a inner join member b on a.user_id = b.user_id
</select>
</mapper>